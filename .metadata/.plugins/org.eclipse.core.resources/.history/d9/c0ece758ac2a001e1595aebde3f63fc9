import java.util.Scanner;
public class Series {
	public void FibonacciSeries(int n)
	{
		int fib1=0;
		int fib2=1;
		if(n==1) {
			System.out.println(fib1);
		}
		else if(n==2){
			System.out.println(fib1 +" "+ fib2);
		}
		else
		{
			System.out.println(fib1+" "+fib2+" ");
			for(int i=3;i<=n;j++) {
				int nextNo=fib1+fib2;
				System.out.println(nextNo+" ");
				fib1=fib2;
				fib2=nextNo;
			}
			
		}
	}

}
